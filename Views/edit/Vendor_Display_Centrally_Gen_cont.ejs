<script>
function toggleRadio(element) {
    // Uncheck other radio buttons in the same row
    var rowRadios = document.querySelectorAll('input[name="' + element.name + '"]');
    rowRadios.forEach(function(radio) {
        if (radio !== element) {
            radio.checked = false;
        }
    });
}
</script>         
