   document.getElementById("searchButton").addEventListener("click", function() {
  // Get the search query
  var query = document.getElementById("searchInput").value.toLowerCase();

  // Get all the links in the sidebar
  var links = document.querySelectorAll(".nav-links li a");

  // Loop through the links to find a match
  for (var i = 0; i < links.length; i++) {
    var linkText = links[i].innerText.toLowerCase();

    // Check if the link text contains the search query
    if (linkText.includes(query)) {
      // Navigate to the matched page
      window.location.href = links[i].getAttribute("href");
      return; // Stop searching after the first match is found
    }
  }

  // If no match is found
  alert("No matching page found.");
});

   
